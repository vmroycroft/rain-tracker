{
	"tags": {
		"allowUnknownTags": true
	},
	"source": {
		"include": [
			"./src"
		]
	},
	"plugins": [
		"plugins/markdown",
		"better-docs/category",
		"better-docs/component"
	],
	"opts": {
		"encoding": "utf8",
		"destination": "docs/",
		"recurse": true,
		"verbose": true,
		"template": "./node_modules/better-docs"
	},
	"templates": {
		"better-docs": {
			"name": "Rain Tracker"
		}
	}
}